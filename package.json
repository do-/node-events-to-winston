{
  "name": "events-to-winston",
  "version": "1.0.0",
  "description": "Observing an EventEmitter, logging to winston",
  "main": "index.js",
  "files": [
    "/lib",
    "/badges"
  ],
  "scripts": {
    "test": "jest",
    "test-ci": "jest --ci --coverage"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/**/*.js"
    ],
    "coverageReporters": [
      "clover",
      "json",
      "lcov",
      "text",
      "json-summary"
    ],
    "verbose": false,
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/do-/node-events-to-winston.git"
  },
  "keywords": [
    "events",
    "node",
    "winston"
  ],
  "author": "Dmitry Ovsyanko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/do-/node-events-to-winston/issues"
  },
  "homepage": "https://github.com/do-/node-events-to-winston#readme",
  "peerDependencies": {
    "winston": "^3.14.0"
  },
  "dependencies": {
    "subclassable-object-merger": "^1.0.1"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  }
}
